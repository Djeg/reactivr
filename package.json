{
  "name": "reactivr",
  "version": "0.0.1",
  "description": "A react library for managing state and effects in a reactive way",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/Djeg/reactivr",
  "author": "David Jegat",
  "keywords": [
    "react",
    "functional",
    "reactive",
    "redux",
    "actions",
    "state management",
    "state",
    "effect",
    "side effect"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/ramda": "^0.27.45",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "jest": "^27.2.0",
    "ramda": "^0.27.1",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "dependencies": {
    "react": "^17.0.2"
  }
}
